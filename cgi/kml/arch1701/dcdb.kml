<kml>
    <Folder>
	<name>dcdb arch1701.local</name>
	<visibility>0</visibility>
	<open>1</open>
	<description>just tick the boxes</description>
    
	<NetworkLink>
            <name>Lots</name>
            <visibility>0</visibility>
            <description>Test</description>
            <Link>
                <href>http://arch1701.local/cgi-bin/poly.cgi</href>
                <viewRefreshMode>onStop</viewRefreshMode>
                <viewRefreshTime>1</viewRefreshTime>
                <httpQuery>

                    schema=qspatial              &amp;
                    table=qld_cadastre_dcdb      &amp;
                    index=objectid               &amp;
                    geom=o_shape                 &amp;
                    epsg=7844                    &amp;
                    title=dcdb 2020              &amp;
                    name=lotplan                 &amp;
                    altname=feat_name            &amp; 
                    altname2=alias_name          &amp; 
                    color=ffffffff               &amp;
                    width=2                      &amp;
                    where=parcel_typ ~ '^Lot'

                </httpQuery>
            </Link>
	</NetworkLink>


